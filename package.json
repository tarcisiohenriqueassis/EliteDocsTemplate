{
  "name": "elitedocs",
  "private": true,
  "main": "electron/main.cjs",
  "author": "Elite Support TI",
  "copyright": "© 2025 Elite Support TI. Todos os direitos reservados.",
  "version": "0.1.0",
  "type": "commonjs",
  "scripts": {
    "dev": "npx concurrently \"npx vite\" \"npx wait-on http://localhost:5173 && npx cross-env NODE_ENV=development electron electron/main.cjs\"",
    "build": "vite build",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.EliteDocs.gerador",
    "productName": "EliteDocs",
    "copyright": "© 2025 Elite Support TI. Todos os direitos reservados.",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json",
      "public/**/*",
      "modelos/**/*",
      "templates/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "extraResources": [
      {
        "from": "templates/",
        "to": "templates/",
        "filter": ["**/*"]
      }
    ]
  },
  "dependencies": {
    "docx": "^9.5.1",
    "docxtemplater": "^3.66.2",
    "file-saver": "^2.0.5",
    "html2pdf.js": "^0.12.0",
    "jszip": "^3.10.1",
    "pizzip": "^3.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-imask": "^7.6.1",
    "react-input-mask": "^2.0.4",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  }
}
